{
  "name": "@trillionclues/contour",
  "version": "1.0.0",
  "description": "Professional API mock server from OpenAPI specifications",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "contour": "dist/cli/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx watch src/cli/index.ts",
    "build": "tsc",
    "build:prod": "pnpm run clean && tsc",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build:prod && pnpm test"
  },
  "keywords": [
    "api",
    "mock",
    "mock-server",
    "openapi",
    "swagger",
    "testing",
    "development",
    "fake-data",
    "faker",
    "cli",
    "rest-api",
    "api-mocking"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "trillionclues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/trillionclues/contour.git"
  },
  "bugs": {
    "url": "https://github.com/trillionclues/contour/issues"
  },
  "homepage": "https://github.com/trillionclues/contour#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.6.7",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "ora": "^8.0.1",
    "swagger2openapi": "^7.0.8",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.16",
    "@types/swagger2openapi": "^7.0.4",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.10",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}